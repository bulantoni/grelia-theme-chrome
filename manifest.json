{
    "description": "Google Chrome Theme",
    "version": "1.0",
    "manifest_version": 2,
    "name": "Grelia",
    "theme": {
        "colors": {
            "frame": [191, 191, 191],
            "frame_inactive": [191, 191, 191],
            "frame_incognito": [200, 200, 200],
            "frame_incognito_inactive": [200, 200, 200],
            "toolbar": [200, 200, 200],
            "tab_text": [77, 77, 77],
            "tab_background_text": [77, 77, 77],
            "bookmark_text": [77, 77, 77],
            "nt_header": [255, 255, 255],
            "ntp_background": [245, 245, 245],
            "ntp_text": [245, 245, 245],
            "button_background": [174, 174, 174, 0.1],
